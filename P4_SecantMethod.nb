Cell[BoxData[{
 RowBox[{"ClearAll", ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"SecantMethod", "[", 
    RowBox[{"ao_", ",", "bo_", ",", "m_"}], "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", "[", " ", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"a", "=", 
        RowBox[{"N", "[", "ao", "]"}]}], ",", 
       RowBox[{"b", "=", 
        RowBox[{"N", "[", "bo", "]"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"k", "=", "0"}], ";", "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"k", "<", "m"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"f", "[", "a", "]"}], "==", 
            RowBox[{"f", "[", "b", "]"}]}], ",", 
           RowBox[{
            RowBox[{"Print", "[", "\"\<Same root will print\>\"", "]"}], 
            "\[IndentingNewLine]", 
            RowBox[{"Break", "[", "]"}]}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"c", "=", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{
                RowBox[{"a", "*", 
                 RowBox[{"f", "[", "b", "]"}]}], " ", "-", 
                RowBox[{"b", "*", 
                 RowBox[{"f", "[", "a", "]"}]}]}], ")"}], "/", 
              RowBox[{"(", 
               RowBox[{
                RowBox[{"f", "[", "b", "]"}], "-", 
                RowBox[{"f", "[", "a", "]"}]}], ")"}]}]}], ";", 
            "\[IndentingNewLine]", "\[IndentingNewLine]", 
            RowBox[{"Print", "[", 
             RowBox[{"\"\<value at \>\"", ",", 
              RowBox[{"k", "+", "1"}], ",", "\"\< th iteration is = \>\"", 
              ",", 
              RowBox[{"NumberForm", "[", 
               RowBox[{"c", ",", "16"}], "]"}]}], "]"}], ";", 
            "\[IndentingNewLine]", 
            RowBox[{"a", "=", "b"}], ";", "\[IndentingNewLine]", 
            RowBox[{"b", "=", "c"}], ";"}]}], "\[IndentingNewLine]", 
          "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
         RowBox[{"k", "=", 
